{
    "name": "drupal/analyze_ai_content_security_audit",
    "description": "Analyzes content on security vectors using AI.",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.1",
        "drupal/core": "^10.2 || ^11",
        "drupal/analyze": "^1.0",
        "drupal/ai": "^1.0",
        "drupal/views_color_scales": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "mglaman/phpstan-drupal": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
} 